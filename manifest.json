{
  "name": "Tab Monkey",
  "version": "0.2.3",
  "manifest_version": 2,
  "description": "Quick tab navigation. Jump to last tab (previous tab), bookmark tabs, jump jump!",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "background": {
      "scripts": ["background.js"]
  },
  "options_page": "options.html",
  "permissions": [
    "tabs",
    "notifications"
  ],
  "web_accessible_resources": [
    "icon.png"
  ]
}
